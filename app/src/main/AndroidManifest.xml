<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.urdataa">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/logourdata"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logourdata"
        android:supportsRtl="true"
        android:theme="@style/Theme.UrDataa"
        android:usesCleartextTraffic="true">

        <activity android:name=".ui.user.UserEventActivity" />
        <activity android:name=".ui.user.UserDetailActivity" />
        <activity android:name=".ui.splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.login.LoginActivity" />


        <activity android:name=".Adapter.UserAdapter" />
        <activity android:name=".ui.MainActivity"></activity>
    </application>

</manifest>